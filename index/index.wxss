.user-card-bg{
  width: 100%;
  height: 514rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: linear-gradient(to bottom,#66a6ff,#80e2f3);

}
/*scroll-view选择模板样式*/
.bg-select-title{
  padding: 20rpx;
  font-size: 28rpx;
  color:#666666;
  background: #fff;
}
.card-bg-scroll{
  width: 690rpx;
  padding: 30rpx 0;
  white-space: nowrap;
  margin: 0 auto;
  border-top: 1px solid #e5e5e5;
  border-bottom: 1px solid #e5e5e5;
}
.card-bg-item{
  width: 152rpx;
  height: 84rpx;
  margin-right: 30rpx;
  display: inline-block;
  border-radius: 6px;
  border: 4rpx solid #fff;
}
.card-bg-item>image{
  width: 152rpx;
  height: 84rpx;
  box-shadow: 0 0 3rpx 5rpx #e5e5e5;
}
.bg-selected{
  border-color:#256aff;
}
.bg-selected>image{
  box-shadow: none;
}

.mb-tip{
  color: #666666;
  font-size: 28rpx;
  padding: 14rpx 30rpx 14rpx 30rpx;
}