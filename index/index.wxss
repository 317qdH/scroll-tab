/*名片显示模板样式*/
.u-card-box{
  width: 712rpx;
  height: 436rpx;
  display: flex;
  flex-direction: column;
  position: relative;
  z-index: 10;
}
.u-card-box .uc-bg{
  width: 712rpx;
  height: 436rpx;
  position: absolute;
  left: 0;
  top: 0;
}
.u-card-box>.uc-avatar{
  overflow: hidden;
}
.u-card-box .uc-title>view{
  font-size: 32rpx;
  font-weight: bold;
}
.u-card-box .uc-title>text{
  font-size: 24rpx;
}
.u-card-box .uc-infor .us-company-name{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  background: url(http://img0.912688.com/mc-card-bg-icons.png) no-repeat;
  background-size: 30rpx 1626rpx;  
}
.u-card-box .uc-infor .us-iphone{
  background: url(http://img0.912688.com/mc-card-bg-icons.png) no-repeat;
  background-size: 30rpx 1626rpx; 
}
.u-card-box .uc-infor .us-email{
  background: url(http://img0.912688.com/mc-card-bg-icons.png) no-repeat;
  background-size: 30rpx 1626rpx; 
}

